{
  "name": "mcbbs-client",
  "author": "langyo",
  "version": "0.2.5",
  "description": "A BBS client for MCBBS.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/langyo/MCBBS-Client/issues"
  },
  "homepage": "https://github.com/langyo/MCBBS-Client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/langyo/MCBBS-Client.git"
  },
  "scripts": {
    "electron:start": "cd native/electron/ && npm run start",
    "electron:watch": "cd native/electron/ && npm run watch",
    "electron:build": "cd native/electron/ && npm run build",
    "electron:package": "cd native/electron/ && npm run package",
    "electron:package:windows": "cd native/electron/ && npm run package:windows",
    "electron:package:mac": "cd native/electron/ && npm run package:mac",
    "electron:test": "cd native/electron/ && npm run test",
    "ionic:ng": "cd native/ionic/ && npm run ng",
    "ionic:start": "cd native/ionic/ && npm run start",
    "ionic:build": "cd native/ionic/ && npm run build",
    "ionic:test": "cd native/ionic/ && npm run test",
    "ionic:lint": "cd native/ionic/ && npm run lint",
    "ionic:e2e": "cd native/ionic/ && npm run e2e",
    "install": "cd native/electron/ && npm i && cd ../../native/ionic && npm i",
    "install:cnpm": "cd native/electron/ && cnpm i && cd ../../native/ionic && cnpm i",
    "install:fix": "cd native/electron/ && npm audit fix --force && cd ../../native/ionic && npm audit fix --force"
  }
}
